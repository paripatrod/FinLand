# Backend Environment Variables

# Flask Configuration
FLASK_ENV=production
FLASK_DEBUG=0
SECRET_KEY=your-super-secret-key-change-this-in-production

# CORS Settings  
CORS_ORIGINS=http://localhost:5173,http://localhost:4173,https://your-production-domain.com

# Model Configuration (HuggingFace - Auto Download)
MODEL_URL=https://huggingface.co/Pottersk/finland-ai-model/resolve/main/financial_advisor_model.pkl
MODEL_VERSION=3.0.0

# API Rate Limiting (optional)
RATE_LIMIT_PER_MINUTE=60

# Logging
LOG_LEVEL=INFO
